<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>hotel</display-name>
  <servlet>
    <servlet-name>FoodTypeServlet</servlet-name>
    <servlet-class>youshan.com.servlet.FoodTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DinnerTableServlet</servlet-name>
    <servlet-class>youshan.com.servlet.DinnerTableServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FoodServlet</servlet-name>
    <servlet-class>youshan.com.servlet.FoodServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>FoodTypeServlet</servlet-name>
    <url-pattern>/FoodTypeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DinnerTableServlet</servlet-name>
    <url-pattern>/DinnerTableServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FoodServlet</servlet-name>
    <url-pattern>/FoodServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>sys/index.jsp</welcome-file>
 
  </welcome-file-list>
</web-app>